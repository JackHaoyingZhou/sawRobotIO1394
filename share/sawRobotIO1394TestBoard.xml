            <BitsToVolts Scale="0.0000686656" Offset="0"/>
            <VoltsToPosSI Scale="1" Offset="0"/>
        </AnalogIn>
    </Actuator>
    <Coupling Value="1">
        <ActuatorToJoint>
            <Row Val="0 1 0 0"/>
            <Row Val="1 0 0 0"/>
            <Row Val="0 0 1 1"/>
            <Row Val="0 0 0 1"/>
        </ActuatorToJoint>
        <JointToActuator>
            <Row Val="0 1 0 0"/>
            <Row Val="1 0 0 0"/>
            <Row Val="0 0 1 -1"/>
            <Row Val="0 0 0 1"/>
        </JointToActuator>

        <ActuatorTorqueToJointTorque>
            <Row Val="0 1 0 0"/>
            <Row Val="1 0 0 0"/>
            <Row Val="0 0 1 1"/>
            <Row Val="0 0 0 1"/>
        </ActuatorTorqueToJointTorque>

        <JointTorqueToActuatorTorque>
            <Row Val="0 1 0 0"/>
            <Row Val="1 0 0 0"/>
            <Row Val="0 0 1 -1"/>
            <Row Val="0 0 0 1"/>
        </JointTorqueToActuatorTorque>
    </Coupling>

  </Robot>

